@using MvcOnlineTicariOtomasyon.Models.Siniflar
@model List<KargoTakip>
@{
    ViewBag.Title = "KargoTakip";
    Layout = "~/Views/Shared/_AdminLayout2.cshtml";
}

<body>
    <div class="page-wrapper">
        <!-- Page Content-->
        <div class="page-content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12 col-sm-12">
                        <div class="card">
                            <div class="card-body table-responsive">
                                <h5 class="header-title">Kargo Hareketleri</h5>
                                <table id="#" class="table table-bordered table-hover" style="margin-top:20px;">
                                    <thead>
                                        <tr class="table-active">
                                            <th>Açıklama</th>
                                            <th>Tarih</th>

                                        </tr>
                                    </thead>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>@item.Aciklama</td>
                                            <td>@item.TarihZaman</td>

                                        </tr>
                                    }
                                </table>                             
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>

